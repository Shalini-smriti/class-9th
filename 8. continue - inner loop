public class Main {
    public static void main(String[] args) {

        for (int i = 1; i <= 3; i++) {
            System.out.println("Outer loop i = " + i);

            for (int j = 1; j <= 3; j++) {
                if (j == 2) {
                    continue;  // skips j == 2
                }
                System.out.println("  Inner loop j = " + j);
            }
        }
    }
}


Outer loop i = 1
  Inner loop j = 1
  Inner loop j = 3
Outer loop i = 2
  Inner loop j = 1
  Inner loop j = 3
Outer loop i = 3
  Inner loop j = 1
  Inner loop j = 3
