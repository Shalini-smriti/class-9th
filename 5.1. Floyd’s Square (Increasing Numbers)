import java.util.Scanner;

public class FloydsSquare {
    public static void main(String[] args) {
    
        int num = 1;

        for (int i = 1; i <= n; i++) {        // outer loop for rows
            for (int j = 1; j <= n; j++) {    // inner loop for columns
                System.out.print(num + " ");
                num++;
            }
            System.out.println();
        }
    }
}


