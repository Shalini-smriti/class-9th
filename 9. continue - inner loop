public class Main {
    public static void main(String[] args) {

        for (int i = 1; i <= 5; i++) {

            if (i == 3) {
                continue; // skips entire iteration when i == 3
            }

            System.out.println("Outer loop i = " + i);

            for (int j = 1; j <= 3; j++) {
                System.out.println("  Inner loop j = " + j);
            }
        }
    }
}


Outer loop i = 1
  Inner loop j = 1
  Inner loop j = 2
  Inner loop j = 3
Outer loop i = 2
  Inner loop j = 1
  Inner loop j = 2
  Inner loop j = 3
Outer loop i = 4
  Inner loop j = 1
  Inner loop j = 2
  Inner loop j = 3
Outer loop i = 5
  Inner loop j = 1
  Inner loop j = 2
  Inner loop j = 3
