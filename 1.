public class Main {
    public static void main(String[] args) {
        for (int i = 1; i <= 3; i++) {          // outer loop → rows
            for (int j = 1; j <= 5; j++) {      // inner loop → columns
                System.out.print("* ");
            }
            System.out.println();               // move to next line
        }
    }
}





/*
output

* * * * *
* * * * *
* * * * *

*/
